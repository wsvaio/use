{
  "name": "@wsvaio/use",
  "type": "module",
  "version": "1.0.6",
  "description": "常用vue组合api",
  "author": "wsvaio <wsvaio@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/wsvaio",
  "homepage": "https://github.com/wsvaio/use#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsvaio/use.git"
  },
  "bugs": {
    "url": "https://github.com/wsvaio/use/issues"
  },
  "keywords": [
    "wsvaio",
    "vue",
    "composable",
    "use"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite ./playground",
    "test": "vitest",
    "build": "tsup",
    "commit": "czg",
    "prepublishOnly": "npm run build",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@wsvaio/utils": "latest"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^4.2.3",
    "@wsvaio/commitlint-config": "latest",
    "@wsvaio/eslint-config": "latest",
    "conventional-changelog": "^3.1.25",
    "simple-git-hooks": "^2.8.1",
    "czg": "^1.6.1",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.9"
  }
}
